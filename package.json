{
  "name": "madame-sdk",
  "version": "1.0.16",
  "description": "",
  "main": "lib/Api.js",
  "types": "lib/Api.d.ts",
  "config": {
    "validate-commit-msg": {
      "types": [
        "build",
        "chore",
        "ci",
        "cleanup",
        "docs",
        "feat",
        "fix",
        "followup",
        "improve",
        "perf",
        "refactor",
        "revert",
        "style",
        "test",
        "unfeat"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@crazyfactory/tinka": "^1.8.2",
    "@types/jest": "^24.0.11",
    "autobind-decorator": "^2.4.0",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0"
  },
  "devDependencies": {
    "@crazyfactory/tslint-rules": "^1.6.1",
    "husky": "^1.3.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.3.4000",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "test": "jest",
    "test:no-cache": "jest --no-cache",
    "test:watch": "jest --watch",
    "lint": "tslint -t stylish --project tsconfig.json \"src/**/**.ts*\" -e \"src/**/**.snap\"",
    "lint:fix": "npm run lint -s -- --fix",
    "prepublish": "npm run test && tsc && npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madameorganic-dev/sdk.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/madameorganic-dev/sdk/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "cacheDirectory": "<rootDir>/.jest/cache",
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "./"
    ]
  },
  "homepage": "https://github.com/madameorganic-dev/sdk#readme"
}
